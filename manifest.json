{
    "manifest_version": 3,
    "name": "E/D Extension",
    "version": "1.01",
    "description": "A simple Chrome extension for encrypting and decrypting messages.",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "ED_icon.png",
