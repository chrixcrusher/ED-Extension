{
    "manifest_version": 3,
    "name": "E/D Extension",
    "version": "1.0",
    "description": "A simple Chrome extension for encrypting and decrypting messages.",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon_2.png",
        "128": "icon_2.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "48": "icon_1.png",
      "128": "icon_2.png"
    }
  }
  